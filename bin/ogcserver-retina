#!/bin/bash
gunicorn \
       "ogcserver.wsgi:WSGIApp('/conf/ogcserver-osm.conf', '/data/osm-crisp/osm-mapnik-retina.xml')" \
       --access-logfile=/logs/retina-osm-access.log \
       --error-logfile=/logs/retina-osm-error.log \
       --timeout=300  \
       -k eventlet \
       -w 6 \
       -b 0.0.0.0:80
